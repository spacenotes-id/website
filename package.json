{
  "name": "spacenotes-frontend-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "commit": "cz",
    "format": "prettier -w -c ./.prettierrc.js \"./src/**/*.{js,jsx,ts,tsx,css}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@tanstack/react-query": "^4.33.0",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "htmr": "^1.0.2",
    "jotai": "^2.4.1",
    "lucide-react": "^0.268.0",
    "marked": "^8.0.1",
    "nanoid": "^4.0.2",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "next-themes": "^0.2.1",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "tailwind-merge": "^1.14.0",
    "ts-pattern": "^5.0.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "20.5.2",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "autoprefixer": "10.4.15",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint",
    "**/*.{js,jsx,ts,tsx,mdx,md,css}": [
      "prettier -w -c ./.prettierrc.js \"./src/**/*.{js,jsx,ts,tsx,css}\""
    ]
  },
  "browserslist": {
    "production": [
      ">0.01%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.01%",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
